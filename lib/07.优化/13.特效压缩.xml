<?xml version="1.0"?>
<dialog title="特效压缩" buttons="accept, cancel">
    <vbox>
        <label value="抽帧压缩-模式选择："/>
        <radiogroup id="frameCompressionGroup">
            <hbox>
                <radio label="不启用" value="none"/>
                <radio label="隔二删一" value="skip2delete1"/>
                <radio label="隔一删一" value="skip1delete1"/>
                <radio label="隔一删二" value="skip1delete2"/>
            </hbox>
        </radiogroup>
        <label value="像素压缩-模式选择（比较耗时）："/>
        <radiogroup id="pixelCompressionGroup">
            <hbox>
                <radio label="不启用" value="1.0"/>
                <radio label="压缩35%" value="0.65"/>
                <radio label="压缩50%" value="0.5"/>
                <radio label="压缩65%" value="0.35"/>
            </hbox>
        </radiogroup>
        <separator/>
    </vbox>
</dialog>